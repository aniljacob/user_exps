postinstall scriptlet (using /bin/sh):
# kernel-3.12.37-rt51+g43cecda - postinst
	if [ ! -e "$D/lib/modules/3.12.37-rt51+g43cecda" ]; then
		mkdir -p $D/lib/modules/3.12.37-rt51+g43cecda
	fi
	if [ -n "$D" ]; then
		depmodwrapper -a -b $D 3.12.37-rt51+g43cecda
	else
		depmod -a 3.12.37-rt51+g43cecda
	fi
