postinstall scriptlet (using /bin/sh):
# net-snmp-server - postinst
#!/bin/sh
if type update-rc.d >/dev/null 2>/dev/null; then
	if [ -n "$D" ]; then
		OPT="-r $D"
	else
		OPT="-s"
	fi
	update-rc.d $OPT snmpd start 90 2 3 4 5 . stop 60 0 1 6 .
fi
