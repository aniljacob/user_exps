Name: packagegroup-fsl-benchmark-core
Name: gdk-pixbuf-loader-jpeg
postinstall scriptlet (using /bin/sh):
# gdk-pixbuf-loader-jpeg - postinst
#!/bin/sh
if [ "x$D" != "x" ]; then
	$INTERCEPT_DIR/postinst_intercept update_pixbuf_cache gdk-pixbuf-loader-jpeg mlprefix= libdir=/usr/lib \
		bindir=/usr/bin base_libdir=/lib
else

	# Update the pixbuf loaders in case they haven't been registered yet
	GDK_PIXBUF_MODULEDIR=/usr/lib/gdk-pixbuf-2.0/2.10.0/loaders gdk-pixbuf-query-loaders --update-cache

	if [ -x /usr/bin/gtk-update-icon-cache ] && [ -d /usr/share/icons ]; then
		for icondir in /usr/share/icons/*; do
			if [ -d ${icondir} ]; then
				gtk-update-icon-cache -t -q ${icondir}
			fi
		done
	fi
fi
