/opt/fsl-qoriq/f140/1.9/sysroots/x86_64-fslsdk-linux/usr/bin/rpm
Name: netbase
Name: kexec-tools
Name: lmsensors-config-libsensors
Name: fbset-modes
Name: busybox
postinstall scriptlet (using /bin/sh):
# busybox - postinst
	# This part of code is dedicated to the on target upgrade problem.
	# It's known that if we don't make appropriate symlinks before update-alternatives calls,
	# there will be errors indicating missing commands such as 'sed'.
	# These symlinks will later be updated by update-alternatives calls.
	test -n 2 > /dev/null || alias test='busybox test'
	if test "x$D" = "x"; then
		# Remove busybox.nosuid if it's a symlink, because this situation indicates
		# that we're installing or upgrading to a one-binary busybox.
		if test -h /bin/busybox.nosuid; then
			rm -f /bin/busybox.nosuid
		fi
		for suffix in "" ".nosuid" ".suid"; do
			if test -e /etc/busybox.links$suffix; then
				while read link; do
					if test ! -e "$link"; then
						case "$link" in
							/*/*/*)
								to="../../bin/busybox$suffix"
								;;
							/bin/*)
								to="busybox$suffix"
								;;
							/*/*)
								to="../bin/busybox$suffix"
								;;
						esac
						# we can use busybox here because even if we are using splitted busybox
						# we've made a symlink from /bin/busybox to /bin/busybox.nosuid.
						busybox rm -f $link
						busybox ln -s $to $link
					fi
				done < /etc/busybox.links$suffix
			fi
		done
	fi
	update-alternatives --install /usr/bin/ar ar /bin/busybox 50
	update-alternatives --install /bin/gunzip gunzip /bin/busybox 50
	update-alternatives --install /bin/zcat zcat /bin/busybox 50
	update-alternatives --install /usr/bin/bunzip2 bunzip2 /bin/busybox 50
	update-alternatives --install /usr/bin/bzcat bzcat /bin/busybox 50
	update-alternatives --install /usr/bin/unlzma unlzma /bin/busybox 50
	update-alternatives --install /usr/bin/lzcat lzcat /bin/busybox 50
	update-alternatives --install /usr/bin/lzma lzma /bin/busybox 50
	update-alternatives --install /usr/bin/unxz unxz /bin/busybox 50
	update-alternatives --install /usr/bin/xzcat xzcat /bin/busybox 50
	update-alternatives --install /usr/bin/xz xz /bin/busybox 50
	update-alternatives --install /usr/bin/bzip2 bzip2 /bin/busybox 50
	update-alternatives --install /bin/cpio cpio /bin/busybox 50
	update-alternatives --install /bin/gzip gzip /bin/busybox 50
	update-alternatives --install /bin/lzop lzop /bin/busybox 50
